<Settings>
  <Sitecore>
    <XConnect>
      <MarketingAutomation>
        <Engine>
          <Services>
            <HabitatFitness.EventNotificationService>
              <Type>Sitecore.HabitatHome.Fitness.Automation.Services.EventNotificationService, Sitecore.HabitatHome.Fitness.Automation</Type>
              <As>Sitecore.HabitatHome.Fitness.Automation.Services.IEventNotificationService, Sitecore.HabitatHome.Fitness.Automation</As>
              <LifeTime>Singleton</LifeTime>
              <Options>
                <FirebaseMessagingApiUri>https://fcm.googleapis.com/fcm/send</FirebaseMessagingApiUri>
                <FirebaseMessagingApiKey>INSERT-SERVER-API-KEY-HERE</FirebaseMessagingApiKey>
                <PublicHostName>https://app.habitathomedemo.com</PublicHostName>
              </Options>
            </HabitatFitness.EventNotificationService>
          </Services>
        </Engine>
      </MarketingAutomation>
    </XConnect>
  </Sitecore>
</Settings>